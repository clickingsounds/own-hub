local MainSection = Main:NewSection("Mining Simulator 2")
MainSection:NewButton("Mining Simulator 2", "Executes the Mining Simulator 2 script", function()
loadstring(game:HttpGet("https://script.vestrahub.xyz",true))()
end)


-- Sliders
SlidersSection:NewSlider("Speed", "Makes you go fast!", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

SlidersSection:NewSlider("Jump", "Makes you jump high!", 500, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

-- Toggles.
TogglesSection:NewToggle("SuperMan", "Makes your become SuperMan :)", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 120
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 120
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

TogglesSection:NewToggle("Speed", "Makes you walk fast", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 120
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

TogglesSection:NewToggle("Jump", "Makes Your jump high", function(state)
    if state then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 120
    else
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 16
    end
end)

TogglesSection:NewToggle("Chat spam", ".", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/clickingsounds/chat-spam./main/on"))()    
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/clickingsounds/off/main/off"))()
    end
end)

-- Hubs 
