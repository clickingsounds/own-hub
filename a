    loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/DomainX/main/source',true))()
end)

-- Chat spam 
ChatSection:NewButton("Have a great day!", "be nice :)", function()
    local A_1 = "Have a great day!"
local A_2 = "All"
local Event = Game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
while true do
wait(0.1)
Event:FireServer(A_1, A_2)
end
end)

ChatSection:NewButton("ez kid", "make kids mad :)", function()
    local A_1 = "ez kid"
local A_2 = "All"
local Event = Game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
while true do
wait(0.1)
Event:FireServer(A_1, A_2)
end
end)

