local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Synapse V3", "Serpent")

local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Pet Simulator X")
local Sliders = Window:NewTab("Sliders")
local SlidersSection = Sliders:NewSection("Sliders")
local Toggles = Window:NewTab("Toggles")
local TogglesSection = Toggles:NewSection("Toggles")
local Hubs = Window:NewTab("Hubs")
local HubsSection = Hubs:NewSection("Hubs")
local Chat = Window:NewTab("Chat")
local ChatSection = Chat:NewSection("Chat Spam")
local FE = Window:NewTab("FE")
local FESection = FE:NewSection("FE scripts")
local DaHood = Window:NewTab("Da Hood")
local DaHoodSection = DaHood:NewSection("Silent Aim")
local Keybind = Window:NewTab("Keybind")
local KeybindSection = Keybind:NewSection("Keybind")
local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc")

MainSection:NewButton("Pet Simulator X", "Executes the Pet Simulator X script", function()
    loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/pet_sim_x_lk.lua")()
end)

local MainSection = Main:NewSection("Blox Fruits")
